.imgList{
  width: 100vw;
  height: 300rpx;
  position: relative;
}

.imgList .imglist{
  width: 100%;
  height: 100%;
}

.content{
  position: absolute;
  bottom: 0;
  left: 0;
  color: white;
  font-size: 26rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}